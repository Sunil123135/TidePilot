{
  "name": "tidepilot",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "db:generate": "pnpm --filter @tidepilot/db exec prisma generate",
    "db:push": "pnpm --filter @tidepilot/db exec prisma db push",
    "db:seed": "pnpm --filter @tidepilot/db exec prisma db seed",
    "db:reset": "pnpm --filter @tidepilot/db exec prisma migrate reset --force",
    "db:studio": "pnpm --filter @tidepilot/db exec prisma studio"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=18"
  }
}
